{
  "name": "@trustify-ui/server",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "bunx rimraf ./dist",
    "clean:all": "bunx rimraf ./dist ./node_modules",
    "check": "biome check .",
    "check:write": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "prebuild": "bun run clean",
    "build": "NODE_ENV=production bun build ./src/index.ts --outdir ./dist --target node --format esm --sourcemap",
    "start:dev": "NODE_ENV=development bun --watch ./src/index.ts",
    "start": "bun run build && bun --sourcemap dist/index.js"
  },
  "dependencies": {
    "@trustify-ui/common": "workspace:*",
    "@types/express": "^4.17.21",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "http-terminator": "^3.2.0",
    "http-proxy-middleware": "^2.0.6"
  },
   "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "typescript": "~5.7.3"
  }, 
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/jest": "^29.5.4",
    "@types/node": "^22.8.1",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw-storybook-addon": "^2.0.4",
    "rimraf": "^6.0.1",
    "storybook": "^8.4.7",
    "storybook-react-rsbuild": "^0.1.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}